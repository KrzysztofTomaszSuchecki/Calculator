   @model IEnumerable<CalculatorDataLayer.Model.Operation>

@{ ViewData["Title"] = "Getall"; }

<title>Calculator</title>

<p>
    <a asp-action="Calculator">Kalkulator</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.No1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.No2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Result)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OperaionTypeId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OperationDateTime)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.No1)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.No2)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Result)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.OperaionTypeId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.OperationDateTime)
    </td>
    <td>
        <a asp-action="Delete" asp-controller="Home" asp-route-id="@item.Id">Delete</a>
    </td>
</tr>
}
    </tbody>
</table>
